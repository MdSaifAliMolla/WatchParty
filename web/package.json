    "@heroicons/react": "^2.0.18",
    "@supabase/auth-helpers-nextjs": "^0.8.1",
    "@supabase/supabase-js": "^2.38.0",
    "@vercel/analytics": "^1.2.2",
    "@voxeet/voxeet-web-sdk": "^3.11.1",
    "framer-motion": "^10.16.4",
    "isomorphic-ws": "^4.0.1",
    "next": "^13.5.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.0.0",
    "react-uuid": "^1.0.2",
    "swr": "^2.2.4",
    "ws": "^8.0.0"
  },
  "devDependencies": {
    "@types/react": "18.0.25",
