    "@types/react-dom": "18.0.9",
    "autoprefixer": "^10.4.16",
    "eslint": "7.32.0",
    "eslint-config-next": "^13.5.4",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "typescript": "5.4.3"
