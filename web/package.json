{
  "name": "fireplace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "clean": "node -e \"require('fs').rmSync('.next',{recursive:true,force:true})\"",
    "dev": "node -e \"process.env.NODE_ENV='development';require('child_process').spawn('next',['dev'],{stdio:'inherit',shell:true});\"",
    "dev:clean": "npm run clean && npm run dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.427.0",
